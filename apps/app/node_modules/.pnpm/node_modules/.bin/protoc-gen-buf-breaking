#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/@bufbuild+buf@1.28.1/node_modules/@bufbuild/buf/bin/node_modules:/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/@bufbuild+buf@1.28.1/node_modules/@bufbuild/buf/node_modules:/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/@bufbuild+buf@1.28.1/node_modules/@bufbuild/node_modules:/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/@bufbuild+buf@1.28.1/node_modules:/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/@bufbuild+buf@1.28.1/node_modules/@bufbuild/buf/bin/node_modules:/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/@bufbuild+buf@1.28.1/node_modules/@bufbuild/buf/node_modules:/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/@bufbuild+buf@1.28.1/node_modules/@bufbuild/node_modules:/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/@bufbuild+buf@1.28.1/node_modules:/home/sahildev/Development/Projects/Apotza/Application/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@bufbuild/buf/bin/protoc-gen-buf-breaking" "$@"
else
  exec node  "$basedir/../@bufbuild/buf/bin/protoc-gen-buf-breaking" "$@"
fi
